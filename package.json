{
  "name": "obliterate-implicit-cd",
  "displayName": "Obliterate \"implicit cd\"",
  "description": "This extension obliterate \"implicit cd\" from the world.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ktbarry/vscode-Obliterate_implicit_cd"
  },
  "bugs": {
    "url": "https://github.com/ktbarry/vscode-Obliterate_implicit_cd/issues"
  },
  "homepage": "https://github.com/ktbarry/vscode-Obliterate_implicit_cd#readme",
  "activationEvents": [
    "onLanguage:markdown",
    "onLanguage:asciidoc"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "obliterate-implicit-cd.helloWorld",
        "title": "OIC Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "commander": "^13.0.0"
  }
}
